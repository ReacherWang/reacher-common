package org.reacher.common.utils;

/**
 * @author reacher
 *
 */
public final class ObjectUtil {

	private ObjectUtil() {
	}

	public static boolean equals(Object o1, Object o2) {
		return (o1 == o2) || (o1 != null && o1.equals(o2));
	}
	
	public static boolean notEquals(Object o1, Object o2) {
		return !equals(o1, o2);
	}
}
